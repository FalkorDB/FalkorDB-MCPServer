{
  "name": "falkordb-mcpserver",
  "version": "1.0.0",
  "description": "Model Context Protocol server for FalkorDB",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
    "lint": "eslint . --ext .ts",
    "test": "jest",
    "test:unit": "jest --config jest.config.js",
    "test:integration": "jest --config jest.config.integration.js",
    "test:integration:setup": "docker-compose -f docker-compose.test.yml up -d",
    "test:integration:teardown": "docker-compose -f docker-compose.test.yml down -v",
    "test:integration:full": "npm run test:integration:setup && npm run test:integration && npm run test:integration:teardown",
    "test:all": "npm run test:unit && npm run test:integration:full",
    "test:coverage": "jest --coverage",
    "test:coverage:unit": "jest --config jest.config.js --coverage",
    "test:coverage:integration": "jest --config jest.config.integration.js --coverage",
    "docker:build": "docker build -t falkordb-mcpserver .",
    "docker:run": "docker run -p 3000:3000 falkordb-mcpserver"
  },
  "keywords": [
    "mcp",
    "modelcontextprotocol",
    "falkordb",
    "graph",
    "database"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.11.0",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.8.0",
    "@types/jsonwebtoken": "^9.0.10",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "falkordb": "^6.2.07",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.2.0"
  }
}
